{
  //Токен, полученынй при авторизации приложения.
  "token_key": "",

  //ID приложения
  "app_id": "",

  //Защищенный ключ приложения
  "security_key": "",

  //Таймаут
  "timeout": 5,

  //Версия VK API
  "api_version": 5.24,

  /**
  * Разрешения/права приложения. Перечислять через запятую
  * Подробнее: https://vk.com/dev/permissions
  */
  "scopes":
  [
    "offline",
    ""
  ],

  //======== AntiCaptcha Part ==========

  //Сервис антикапчи. (По умолчанию выключен.)
  "anticaptcha_enabled": false,

  /**
  * Антикапча, сервис, который позволяет справиться с назойливой капчей.
  * Поддержка antigate.com, rucaptcha.ru встроена
  */
  "anticaptcha_service": "rucaptcha.ru",

  //Ключ доступа для работы с сервисом антикапчи
  "captcha_key":"",

  //Сохранение изображений капчи (и не удалять. Если false, будут удаляться после отправления на сервис)
  "captcha_save_images": true,

  //Папка для сохранений изображений капчи. Тажке у папки должны быть права записи. (775,777)
  "captcha_images_folder": "captcha_images"

}
